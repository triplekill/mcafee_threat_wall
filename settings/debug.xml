<settings>
	<text name="logger:level" value="all" />	<!-- all,none,info,warning,error,fatal -->
	<text name="logger:module" value="all" />	<!-- all,none,or numbers (i.e. "0,1,2,3") -->
	<!-- scales world dimensions to local rect. make sure local rect starts at 0,0 -->
	<text name="scale_world_to_fit" value="false" />
<!--	<text name="logger:async" value="true" />	<!-- (true,false) If this is false, then logging is synchronous -->
	<!-- Print out the tree the first time the app runs. -->
	<text name="tree:startup:print" value="false" />
	<!-- Turn on node printing as you navigate the tree -->
	<text name="tree:print" value="false" />
</settings>
